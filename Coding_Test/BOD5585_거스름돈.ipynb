   ],
   "source": [
    "change = 1000 - int(input())\n",
    "yen = [500, 100, 50, 10, 5, 1]\n",
    "cnt = 0\n",
    "for y in yen:\n",
    "    if change < y:\n",
    "        continue\n",
    "    cnt = cnt + change//y\n",
    "    change %= y\n",
    "print(change, cnt)"
   ]
